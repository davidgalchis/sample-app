{
    "components": {
        "my_lambda": {
            "type": "@lambda.function",
            "description": "CloudKommand Tutorial Lambda",
            "timeout": 10,
            "policies": ["@my_policy"]
        },
        "my_policy": {
            "type": "@iam.policy",
            "description": "CloudKommand Tutorial Policy",
            "document": {
                "Version": "2012-10-17",
                "Statement": [{
                    "Sid": "Vis",
                    "Effect": "Allow",
                    "Action": [
                        "lambda:*",
                        "events:*",
                        "dynamodb:*",
                        "sns:*"
                    ],
                    "Resource": "*"
                }]
            }
        },
        "my_rule": {
            "type": "@eventbridge.rule",
            "name": "test1asdlfjasdflkjasdf",
            "schedule_expression": "rate(5 minutes)",
            "targets": {
              "my_lambda": {
                "arn": "@my_lambda:props.arn"
              }
            }
          }
    },
    "repos": {
        "iam": "https://github.com/cloudkommand/iam",
        "lambda": "https://github.com/cloudkommand/lambda",
        "eventbridge": "https://github.com/cloudkommand/eventbridge"
    }
}